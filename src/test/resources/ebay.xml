<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="amazon smoke amazon">
    <parameter name="useCloudEnv" value="false"></parameter>
    <parameter name="envName" value="browserstack"></parameter>
    <parameter name="os" value="linux"></parameter>
    <parameter name="osVersion" value="11"></parameter>
    <parameter name="browserName" value="firefox"></parameter>
    <parameter name="browserVersion" value="103"></parameter>
    <parameter name="url" value="https://www.ebay.com"></parameter>



    <test name="search amazon">
        <classes>
            <class name="eaby.JewerlyTest"></class>
        </classes>
    </test>






</suite>
